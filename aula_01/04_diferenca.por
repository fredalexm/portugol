programa {
  funcao inicio() {
    inteiro num1, num2, num3, num4, diferenca

    escreva("Digite o primeiro valor: ")
    leia(num1)

    escreva("Digite o segundo valor: ")
    leia(num2)

    escreva("Digite o terceiro valor: ")
    leia(num3)

    escreva("Digite o quarto valor: ")
    leia(num4)

    diferenca = (num1 * num2) - (num3 * num4)

    escreva("A diferença do produto entre o primeiro e segundo numeros digitados pelo produto entre o terceiro e quarto valor é: ", diferenca)
  }
}
